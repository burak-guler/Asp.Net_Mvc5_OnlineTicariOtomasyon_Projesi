
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<script src="~/Scripts/jquery-3.4.1.js"></script>
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<br /> <br />
<button class="btn btn-primary" id="btn1">Button1</button>
<button class="btn btn-warning" id="btn2">Button2</button>
<button class="btn btn-danger" id="btn3">Button3</button>
<button class="btn btn-dark" id="btn4">Button4</button>
<button class="btn btn-warning" id="btn5">Button5</button>
<button class="btn btn-info" id="btn6">Button6</button>
<button class="btn btn-outline-primary" id="btn7">Button7</button>
<button class="btn btn-outline-danger" id="btn8">Button8</button>
<button class="btn btn-outline-info" id="btn9">Button9</button>
<button class="btn btn-outline-secondary" id="btn10">Button10</button>

<script>
    $('#btn1').click(function () {
        swal("Hello World!");
    }
    );
</script>

<script>
    $('#btn2').click(function () {
        swal("Başlık","Merhaba Burak Bu alertleri öğrenmen iyi olur..");
    }
    );
</script>

<script>
    $('#btn3').click(function () {
        swal("Good job!", "You clicked the button!", "success");
    }
    );
</script>

<script>
    $('#btn4').click(function () {
        swal({
            title: "Good job!",
            text: "You clicked the button!",
            icon: "success",
            button: "Aww yiss!",
        });
    }
    );
</script>

<script>
    $('#btn5').click(function () {
        swal("Click on either the button or outside the modal.")
            .then((value) => {
                swal(`The returned value is: ${value}`);
            });
    }
    );
</script>

<script>
    $('#btn6').click(function () {
        swal({
            title: "Are you sure?",
            text: "Once deleted, you will not be able to recover this imaginary file!",
            icon: "warning",
            buttons: true,
            dangerMode: true,
        })
            .then((willDelete) => {
                if (willDelete) {
                    swal("Poof! Your imaginary file has been deleted!", {
                        icon: "success",
                    });
                } else {
                    swal("Your imaginary file is safe!");
                }
            });
    }
    );
</script>

